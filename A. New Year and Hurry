#include<bits/stdc++.h>
using namespace std;

int main(){
    
    int n,k;
    cin>>n>>k;

    int lim = 60 * 4;
    int con = lim - k;
    int cnt = 0;
    int sum = 0;
    for(int i = 1; i <=n; i++){
        int sum = sum + 5*i;
        if(sum <= con ){
            cnt++;
        }
        else
        break;
    }

    cout<<cnt++;
    return 0;

}
